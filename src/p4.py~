#!/usr/bin/python
#!encoding: UTF-8

a = float(raw_input('Valor de a: '))
b = float(raw_input('Valor de b: '))

if a != 0:
  x = -b / a
  pint 'Solucion: ' ,x
if a == 0:  #Con un solo = no se iguala solo se asigna
  if (b != 0):
    print 'La ecuación no tiene solución'
  else: #ejercicio 23
    print 'La ecuación tiene infinitas soluciones'